<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyPartController">
    <children>
        <RadioButton fx:id="rModifyInHouse" layoutX="149.0" layoutY="50.0" mnemonicParsing="false" onAction="#onClickModIn" selected="true" text="In-House">
