         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <TextField fx:id="modifyId" disable="true" layoutX="162.0" layoutY="110.0" prefHeight="25.0" prefWidth="100.0" promptText="Auto Generated - Disabled" />
        <TextField fx:id="modifyName" layoutX="162.0" layoutY="150.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="modifyInv" layoutX="162.0" layoutY="190.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="modifyPrice" layoutX="162.0" layoutY="230.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="modifyMax" layoutX="162.0" layoutY="270.0" prefHeight="25.0" prefWidth="100.0" />
        <TextField fx:id="modifyPartInOut" layoutX="162.0" layoutY="310.0" prefHeight="25.0" prefWidth="100.0" />
        <Label layoutX="66.0" layoutY="114.0" text="ID" />
        <Label layoutX="66.0" layoutY="154.0" text="Name" />
        <Label layoutX="66.0" layoutY="194.0" text="Inv" />
        <Label layoutX="66.0" layoutY="234.0" text="Price/Cost" />
        <Label layoutX="66.0" layoutY="274.0" text="Max" />
        <Label fx:id="modifyPartLabel" layoutX="66.0" layoutY="314.0" text="Machine ID" />
        <TextField fx:id="modifyMin" layoutX="333.0" layoutY="270.0" prefHeight="25.0" prefWidth="100.0" />
        <Label layoutX="283.0" layoutY="274.0" text="Min" />
        <Button fx:id="saveModifyINV" layoutX="268.0" layoutY="354.0" mnemonicParsing="false" onAction="#onClickSaveModifyINV" prefHeight="25.0" prefWidth="52.0" text="Save" />
